@article{bruckl_graphical_2019,
	title = {Graphical Location of Seismic Sources Based on Amplitude Ratios},
	volume = {90},
	issn = {0895-0695, 1938-2057},
	doi = {10.1785/0220180216},
	abstract = {Methods for location of seismic sources based on amplitudes provide an alternative to standard methods in cases with high ambient noise and emergent, multiple event, or tremor-type source functions. Numerical methods using amplitudes or amplitude ratios have been described and successfully applied in several previous studies. In this article, a graphical location method based on amplitude ratios is presented, which may be of interest to students and others who want to learn about seismology due of its computational simplicity and the clarity of the solution.},
	pages = {784--789},
	number = {2},
